<%= form_for(User.new, @post) do |f| %>

  <div class="field">
      <%= f.label 'User' %>
      <%= f.text_field :user_id %>
  </div>
  <div class="field">
    <%= f.label 'Title' %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label 'Body' %><br>
    <%= f.text_area :body %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
